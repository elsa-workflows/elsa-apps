{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Sqlite": "Data Source=elsa.sqlite.db;cache=shared"
  },
  "DatabaseProvider": "Sqlite",
  "Http": {
    "BaseUrl": "https://localhost:9030"
  },
  "Identity": {
    "Tokens": {
      "SigningKey": "large-signing-key-for-signing-JWT-tokens"
    }
  },
  "Resilience": {
    "Strategies": [
      {
        "$type": "HttpResilienceStrategy",
        "Id": "1",
        "DisplayName": "Retry 3 times, exponential",
        "MaxRetryAttempts": 3,
        "BackoffType": "Exponential"
      },
      {
        "$type": "HttpResilienceStrategy",
        "Id": "2",
        "DisplayName": "Retry 10 times, linear",
        "MaxRetryAttempts": 10,
        "BackoffType": "Linear"
      }
    ]
  },
  "LoggingFramework": {
    "Defaults": [
      "Console"
    ],
    "Sinks": [
      {
        "Type": "Console",
        "Name": "Console",
        "Options": {
          "MinLevel": "Trace",
          "CategoryFilters": {
            "Policy": "Information",
            "Policy.Debug": "Debug",
            "Renewal": "Information"
          },
          "Formatter": "Json",
          "IncludeScopes": true,
          "TimestampFormat": "O",
          "DisableColors": true,
          "JsonIndented": false
        }
      }
    ]
  },
  "Dispatcher": {
    "Channels": [
      {
        "Name": "Channel 1"
      },
      {
        "Name": "Channel 2"
      },
      {
        "Name": "Channel 3"
      }
    ]
  }
}
